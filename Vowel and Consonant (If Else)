#include<stdio.h>
main()
{
    char ch;
    printf("Enter character: ");
    scanf("%c",&ch);
    if (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
    {
        printf("Entered character is Vowel");
    }
    else 
    {
        printf("Entered character is Consonant");
    }
}
