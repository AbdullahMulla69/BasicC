#include <stdio.h>
main()
{
    char ch;
    printf("Enter an Alphabet= ");
    scanf("%c", &ch);
    switch (ch)
    {
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':
    printf("Given Alphabet is Vowel");
    break;
    default: printf("Given value is Consonant");
    }
}
