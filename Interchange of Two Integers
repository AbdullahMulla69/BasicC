#include<stdio.h>
main()
{
    int a,b,c;
    printf("Enter first value a= ");
    scanf( "%d"   , &a   );
    printf("Enter second value b= ");
    scanf( "%d"   , &b   );
    printf("Before swapping a= %d and b= %d\n",a,b);
    c=a;
    a=b;
    b=c;
    printf("After swapping a= %d and b= %d",a,b);
}
