#include<stdio.h>
swap(int a, int b)
{
    int x;
    x=a; a=b; b=x;
    printf("After swapping: a= %d and b= %d",a,b);
}
main()
{
    int a,b;
    printf("Enter value of a: ");
    scanf("%d",&a);
    printf("Enter value of b: ");
    scanf("%d",&b);
    printf("Before swapping: a= %d and b= %d\n",a,b);
    swap(a,b);
}
