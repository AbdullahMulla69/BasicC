#include <stdio.h>
 main() 
{
 int age;
  printf("-----Welcome for voting----- \n");
  printf("****************** \n");
 printf("Enter your age= ");
 scanf ( "%d" , &age);
 if (age >= 18)
 {
   printf ("You are eligible for voting\n");   
 }
 else
 {
     printf ("You are NOT eligible for voting\n");
 }
 printf("Thank you\n");
 printf("******************");
}
