#include<stdio.h>
main()
{
    int a,b,c;
    printf("Enter a, b and c: ");
    scanf("%d %d %d", &a,&b,&c);
    if (a>b&&a>c)
    {
        printf("a is the Greatest number");
    }
    else 
    {
        if (b>c)
      {
        printf("b is the Greatest number");
      }
      else
      {
        printf("c is the Greatest number");
      }
    }
}
