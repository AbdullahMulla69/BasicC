#include<stdio.h>
main()
{
    //decleration
    int a[4]; 
    int i;
    //initiation
    for(i=0;i<=3;i++)
    {
        printf("Enter %d roll no.=",i+1);
        scanf("%d",&a[i]);
    }
    //accessing
    for( i=0;i<=3;i++)
    {
        printf("Student %d is",i+1);
        printf("%d",a[i]);
        printf("\n");
    }
}
